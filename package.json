{
  "name": "grafana-plotly-panel",
  "version": "1.0.0",
  "description": "Grafana Plotly Panel",
  "scripts": {
    "build": "grafana-toolkit plugin:build",
    "test": "grafana-toolkit plugin:test",
    "dev": "grafana-toolkit plugin:dev",
    "watch": "grafana-toolkit plugin:dev --watch",
    "sign": "npx @grafana/toolkit plugin:sign",
    "md5": "certutil -hashfile grafana-plotly-panel-%npm_package_version%.zip MD5"
  },
  "author": "nLine",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/preset-env": "7.5.5",
    "@grafana/data": "8.3.4",
    "@grafana/runtime": "8.3.4",
    "@grafana/toolkit": "8.3.4",
    "@grafana/ui": "8.3.4"
  },
  "dependencies": {
    "deepmerge": "*",
    "plotly.js": "2.13.1",
    "react-plotly.js": "*",
    "react-virtualized-auto-sizer": "*"
  },
  "resolutions": {
    "@babel/preset-env": "7.5.5"
  }
}